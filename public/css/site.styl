/**
 * Form control
 */
.form-control
  border-radius 2px
  border none
  border-bottom 1px solid #E0E0E0
  box-shadow 0 1px 5px -2px rgba(0,0,0,.2)
.form-control:focus
  box-shadow 0 1px 5px -2px #42A5F5
  border-bottom 1px solid #cccccc
.form-group
  label
    color: #616161
.form-control-validation
  .help-block,
  .control-label,
  .radio,
  .checkbox,
  .radio-inline,
  .checkbox-inline,
  .form-control.radio label,
  .form-control.checkbox label,
  .form-control.radio-inline label,
  .form-control.checkbox-inline label 
    color #555555
  .form-control
    border-color #cccccc
  .form-control:focus
    border-color darken(#cccccc, 10%)
    box-shadow 0 1px 5px -2px #f5f5f5
  .input-group-addon
    color #555555
    border-color #cccccc
    background-color #f5f5f5
  .form-control-feedback
    color #555555
.input-group-addon,
.input-group-btn
  border none
  border-bottom 1px solid #E0E0E0
  border-radius 0
.has-success
  .form-control-validation
    color #4CAF50
    border-color #4CAF50
    background-color #C8E6C9
.has-warning
  .form-control-validation
    color #FF9800
    border-color #FF9800
    background-color #FFE0B2
.has-error
  .form-control-validation
    color #F44336
    border-color #F44336
    background-color #FFCDD2

#stage
  overflow hidden
  position fixed
  height 100%
  width 100%

.wrapper
  display flex
  #sidebar
    min-width 250px
    max-width 250px
    height 100vh
    background rgba(55,55,55,0.6)
    color #9d9d9d
    transition all 0.3s
    .expanded
      overflow hidden
      white-space nowrap
      width 200px
    a[data-toggle="collapse"]
      position relative
    .sidebar-header
      padding 20px
      background #0a0a0a
      color #efefef
      strong
        display none
      #loginWidget
        font-size 15px
        span
          padding-right 4px
          span
            padding-left 4px
            font-size 16px
    .sidebar-footer
      position absolute
      bottom 8px
      width 250px
      font-size 1.8em
      transition width 200ms linear
      #sidebarCollapse:hover
        color #ffffff
        background #0a0a0a
        transition all 200ms linear
      div
        text-align center
        color #cccccc
        .open
          display block
        .closed
          display none
        div:hover
          color #ffffff
    ul.components
      padding 20px 0
    ul
      p
        color #ffffff
        padding 10px
      li
        a, div
          color #d9d9d9
          padding 10px
          font-size 1.1em
          display block
          text-decoration none
          text-align left
          overflow hidden
          white-space nowrap
          span
            padding-right 5px           
        a:hover, div:hover
          color #ffffff
          background rgba(15,15,15,0.95)
      li.active > a, li.active > div
        color #ffffff
        background #0a0a0a
        span.fa
          text-shadow 0 0 5px #000000
      div[aria-expanded="true"],
      div[aria-expanded="true"]:hover
        color #ffffff
        background #050505 !important
      div[aria-expanded="true"]:hover
        background #252525
      ul
        li
          a
            font-size 0.9em !important
            padding-left 30px !important
            background rgba(25,25,25,0.8)
  #sidebar.active
    min-width 80px
    max-width 80px
    text-align center
    .expanded
      overflow hidden
      white-space nowrap
      width 200px
    .sidebar-header
      h3
        display none
      strong
        display block
      #loginWidget
        span
          span
            display none
    ul
      li
        a, div
          padding 20px 10px
          text-align center
          font-size 1em
          overflow hidden
          white-space nowrap
          span
            margin auto
            display block
            font-size 1.8em
            text-align center
          span.expanded
            display none
      ul
        a
          padding 10px !important
    .sidebar-footer
      position absolute
      bottom 10px
      font-size 1.8em
      width 80px
      span.expanded
        display none
      div
        text-align center
        color #cccccc
        .closed
          display block
        .open
          display none
        div:hover
          color #ffffff

  section.content
    height 100vh
    width 100%
    background rgba(255,255,255,0.95)
    color #121212
    > div > .container,
    > div > .container-fluid
      height 100vh
      overflow-y auto
      margin-left unset
    .login-page
      padding 0 !important
      background-color #121212
      background-image url(/img/background-7-min.jpg)
      background-position center center
      background-repeat no-repeat
      background-attachment fixed
      background-size cover
      height 100%
      > .container-fluid
          background rgba(245,245,245,0.3)
          padding 5px
          height 100vh
      .d-flex
        margin-top 12%
        .card
          padding-left 10px
          padding-right 10px
          width 22rem
          height 13.5rem
          .input-group
            margin-bottom 0.5rem
            .valid
              box-shadow inset 0 -7px 5px -8px #00cc1b
    .content-page
      padding-left 0
      padding-right 0
      overflow hidden
      .page-header
        height 74px
        overflow hidden
        white-space nowrap
        h3
          padding 20px 10px
          padding-right 0
          margin-bottom 0
          background #0a0a0a
          color #ffffff
          height 74px
          font-weight 100
          span, svg
            margin 0 8px
      .page-content
        position relative
        height calc(100vh - 74px)
        overflow-x hidden
        overflow-y auto
        .page-view
          height calc(100vh - 74px)
          overflow-x hidden
          overflow-y auto
          .card-deck
            .card
              margin-bottom 15px
          .part-view
            border-radius 0
            height 100%
            .card-header
              h4
                padding-top 8px
            .card-body
              table
                background #ffffff
                border 0
                tbody
                  td
                    font-weight 400
                    font-size 14px
                    padding 1px 10px
                    border 0
                    .input-group-sm
                      width 225px
                      .disabled
                        color #bcbcbc
                      textarea
                        height 60px
                      select
                        height 30px
                        padding .25rem .25rem
                        line-height 1.2
                      .btn-group-justified
                        width 225px
                        height 30px
                        .btn
                          padding .3rem .7rem
                          font-size 13px
                          width 76px
                          box-sizing border-box
                          background none
                          border none
                          border-bottom 1px solid #e0e0e0
                          box-shadow 0 1px 5px -2px rgba(0,0,0,.2)
                        .btn.active, .btn.active:hover
                          border 1px solid #e0e0e0
                          background rgba(225,225,225,.4)
                          color #121212
                        .btn:hover
                          background rgba(200,200,200,.2)
                          color #616161

          table
            border-top 0
            font-size 13px
            margin-bottom 0
            background rgba(245,245,245,1.0)
            border-bottom 1px solid #e9ecef
            td
              padding .225rem
              -webkit-padding-before .3rem
              -webkit-padding-end .3rem
              -webkit-padding-after .3rem
              -webkit-padding-start .3rem
            tbody
              td
                font-weight 400
            tfoot
              tr,
              tr:hover
                position relative
                background #212529
                color #ffffff
                padding 3px 5px
                .btn-xs
                  padding .05rem .5rem !important
                  font-size 12px !important
                  line-height 1.3 !important
                  border-radius .2rem !important
                  margin 1px
        .bg-7
          background url(/img/background-7-min.jpg) no-repeat
          background-size cover
        .bg-9
          background url(/img/background-9-min.jpg) no-repeat
          background-size cover
        .bg-10
          background url(/img/background-10-min.jpg) no-repeat
          background-size cover
        .bg-11
          background url(/img/background-11-min.jpg) no-repeat
          background-size cover
        .bg-12
          background url(/img/background-12-min.jpg) no-repeat
          background-size cover
        .bg-13
          background url(/img/background-13-min.jpg) no-repeat
          background-size cover
        .bg-14
          background url(/img/background-14.jpg) no-repeat
          background-size cover
        .gray
          filter url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale")
          filter gray
          -webkit-filter grayscale(100%)
        .no-pad
          padding 0
          margin-left 16px
        .action-bar
          background rgba(39,39,39,1)
          border 1px solid #e9ecef
          border-width 0 0 0 1px
          color #ffffff
          height calc(100vh - 74px)
          padding 15px
          margin-right 15px
          width 312px
          min-width 312px
          .card
            cursor default
            .input-group
              margin-bottom 4px
          .btn-group-vertical
            width 100%
            .btn-secondary
              background-color darken(#868e96, 50%)
              border-color darken(#868e96, 50%)
            .btn-secondary:hover
              background-color darken(#727b84, 30%)
              border-color darken(#6c757d, 30%)
            .btn-danger
              background-color darken(#dc3545, 20%)
              border-color darken(#dc3545, 20%)
            .btn-danger:hover
              background-color darken(#c82333, 30%)
              border-color darken(#bd2130, 30%)
